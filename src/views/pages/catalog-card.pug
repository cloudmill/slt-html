  .catalog-page
    section.section.section--first.catalog-page__section 
      .container 
        +breadcrumbs(items = ['SLT AQUA', 'трубы', 'однослойная'] page = "Труба SLT Aqua PP-R SDR 7,4")
        .catalog-page__row 
          .catalog-page__col
            h1.catalog-page__title
              |#[span.word-span #[span(data-aos='fade-up') Труба]] #[span.word-span #[span(data-aos='fade-up') SLT AQUA]] #[span.word-span #[span(data-aos='fade-up') PP-R SDR 7,4]]
            p.catalog-page__text(data-aos='fade-up') 
              |Труба однослойная напорная полипропиленовая PN10 (SDR 11) предназначена для систем холодного и питьевого водоснабжения, а также технологических трубопроводов, транспортирующих жидкости и газы, не агрессивные к материалам трубы.
            .catalog-page__button(data-aos='fade-up')
              .button.button-animate(data-scroll='#order')
                .button__text
                  span
                    |Сделать заказ
                .button__icon 
                  +big-arrow-svg.button__svg
          .catalog-page__col(data-aos='fade-up')
            img.catalog-page__img(src="assets/images/catalog-main.webp", alt="")
          .catalog-page__col
            .catalog-page__label(data-aos='fade-up') 
              |Применение
            ul.catalog-page__types 
              li.catalog-page__types-item(data-aos='fade-up')
                .catalog-page__type 
                  .catalog-page__type-icon.use-icon
                    +connect-svg.use-icon__svg.catalog-page__type-svg
                  .catalog-page__type-text 
                    |Холодное водоснабжение
              li.catalog-page__types-item(data-aos='fade-up')
                .catalog-page__type 
                  .catalog-page__type-icon.use-icon
                    +fire-svg.use-icon__svg.catalog-page__type-svg
                  .catalog-page__type-text 
                    |Горячее водоснабжение
              li.catalog-page__types-item(data-aos='fade-up')
                .catalog-page__type 
                  .catalog-page__type-icon.use-icon
                    +water-svg.use-icon__svg.catalog-page__type-svg
                  .catalog-page__type-text 
                    |Питьевая вода
    section.section.section--md 
      .container
        .catalog-page__container 
          .table 
            .table__grid(data-aos='fade-up')
              -
                var top = [
                  {
                    text: 'Класс #[br] эксплуатации',
                  },
                  {
                    tooltip: true,
                    text: 'Т#[span.table__span раб], ℃',
                  },
                  {
                    text: 'Время при Т#[span.table__span раб], год',
                  },
                  {
                    tooltip: true,
                    text: 'Т#[span.table__span max], ℃',
                  },
                  {
                    text: 'Время при Т#[span.table__span max], год',
                  },
                  {
                    tooltip: true,
                    text: 'Т#[span.table__span авар], час',
                  },
                  {
                    text: 'Время при Т#[span.table__span авар], час',
                  },
                  {
                    text: 'Область #[br] применения',
                  },
                  {
                    tooltip: true,
                    text: 'P#[span.table__span max], МПа',
                  }
                ]
              each item in top
                .table__top 
                  |#{item.text}
                  if (item.tooltip)
                    .tooltip.tooltip--absolute
                      .tooltip__icon 
                        +tooltip-svg.tooltip__svg 
                      .tooltip__container 
                        |Труба однослойная напорная полипропиленовая PN10 (SDR 11) предназначена для систем
              each item in ddata 
                .table__item 
                  ul.table__list 
                    each text in item.items
                      li.table__list-item 
                        .table__text 
                          |#{text}
            .table__mobile(data-aos='fade-up')
              each item in tableMobile 
                if (item.top)
                  .table__top 
                    |#{item.text}
                    if (item.tooltip)
                      .tooltip.tooltip--absolute
                        .tooltip__icon 
                          +tooltip-svg.tooltip__svg 
                        .tooltip__container 
                          |Труба однослойная напорная полипропиленовая PN10 (SDR 11) предназначена для систем
                else 
                  .table__item(class=item.class)
                    ul.table__list 
                      each el in item.items
                        li.table__list-item 
                          .table__text 
                            |#{el}
    section.section 
      .container 
        h3.catalog-page__heading
          |#[span.word-span #[span(data-aos='fade-up') Характеристики]]
        ul.catalog-page__list
          -
            var list = [
              {label: 'Материал', text: '1 слой: PP-R — полипропилен; 2 слой: PP-R-GF — стеклонаполненный полипропилен; 3 слой: PP-R — полипропилен'},
              {label: 'Номинальное давление', text: 'PN10'},
              {label: 'Стандартное размерное соотношение', text: 'SDR'},
              {label: 'Нормализованная серия труб', text: 'S5'},
              {label: 'Коэффициент линейного расширения', text: '0,13 мм/(m×°С)'},
              {label: 'Марка исходного сырья', text: 'SIBEX PP R003 EX/1 (первичный рандом сополимер тип 3)'},
              {label: 'Цвет', text: 'Белый'},
              {label: 'Нормативный документ', text: 'ГОСТ 32415-2013'},
              {label: 'Гарантия', text: '10 лет'},
              {label: 'Срок службы', text: 'Более 50 лет'},
            ] 
          each item in list
            li.catalog-page__list-item(data-aos='fade-up') 
              .catalog-page__list-row 
                .catalog-page__list-col
                  .catalog-page__list-label 
                    |#{item.label}
                .catalog-page__list-col
                  .catalog-page__list-text 
                    |#{item.text}
    section.section.section--md#order
      .container 
        .row 
          .row__col
            h3 
              |#[span.word-span #[span(data-aos='fade-up') Сделать]] #[span.word-span #[span(data-aos='fade-up') заказ]]
          .row__col
            p.catalog-page__caption(data-aos='fade-up') 
              |Здесь вы можете сформировать список требуемых товаров. Наши менеджеры обработают заказ, рассчитают стоимость, найдут подходящего по всем параметрам поставщика и отправят вам коммерческое предложение.
        .catalog-page__colors 
          .catalog-page__colors-item(data-aos='fade-up') 
            .catalog-page__colors-text 
              |Цветовое #[br] исполнение
            .catalog-page__colors-icon.tooltip.active
              +colors-light-svg.catalog-page__colors-svg.tooltip__icon 
              .tooltip__container 
                |белый
            .catalog-page__colors-icon.black.tooltip
              +colors-light-svg.catalog-page__colors-svg.tooltip__icon
              .tooltip__container 
                |черный
            .catalog-page__colors-icon.red.tooltip
              +colors-light-svg.catalog-page__colors-svg.tooltip__icon 
              .tooltip__container 
                |красный
        .section__block
          .catalog-page__table.catalog-page__table--big(data-aos='fade-up')
            .catalog-page__table-head 
              each item in ['Наружный диаметр d#[span.catalog-page__table-span n], мм', 'Артикул', 'Толщина стенки e#[span.catalog-page__table-span n], мм', 'Длина #[br] отрезка, м', 'Количество #[br] в упаковке, м', 'Вес, кг/м', 'Добавить в заказ, шт']
                .catalog-page__table-cell
                  |#{item}
            .catalog-page__table-body
              - for (i = 0; i < 15; i++)
                label.catalog-page__table-item(data-table-item='')
                  each item in ['20', '2SLTPS1120', '1,9', '2', '50', '0,105', 'last']
                    if (item == "last")
                      .catalog-page__table-cell(data-table-cell='')
                        .catalog-page__table-row 
                          .catalog-page__table-col
                            input.catalog-page__table-input(type="number" placeholder="Введите количество" data-table-input='')
                            .catalog-page__table-caption(data-table-caption='')
                              |
                          .catalog-page__table-col
                            .catalog-page__table-button(data-catalog-button='')
                              .catalog-page__table-add 
                                |Добавить
                              .catalog-page__table-icon 
                                +plus-svg.catalog-page__table-svg
                    else
                      .catalog-page__table-cell 
                        .catalog-page__table-text
                          |#{item}
          .catalog-page__block 
            ul.catalog-page__block-list
              -
                var ddata = [
                  {label: "Толщина стенки e#[span.catalog-page__table-span n], мм", text: "1.9"},
                  {label: "Длина отрезка, м", text: "2"},
                  {label: "Количество #[br] в упаковке, м", text: "50"},
                  {label: "Вес, кг/м", text: "0,105"},
                ]
              - for (i = 0; i < 12; i++)
                li.catalog-page__block-item(data-aos='fade-up' data-table-item='') 
                  .catalog-page__block-top 
                    .catalog-page__block-cell 
                      .catalog-page__block-label 
                        |Наружный диаметр d#[span.catalog-page__table-span n], мм
                      .catalog-page__block-text
                        |20
                  .catalog-page__block-body(data-table-cell='')
                    each item in ddata 
                      .catalog-page__block-cell 
                        .catalog-page__block-label 
                          |#{item.label}
                        .catalog-page__block-text
                          |#{item.text}
                    .catalog-page__block-cell 
                      .catalog-page__block-label.darken
                        |Добавить в заказ,шт
                      .catalog-page__block-input
                        input.catalog-page__table-input(type="number" placeholder="Кол-во" data-table-input='')
                        .catalog-page__table-caption(data-table-caption='')
                          |
                    .catalog-page__block-cell 
                      .catalog-page__table-button(data-catalog-button='')
                        .catalog-page__table-add 
                          |Добавить
                        .catalog-page__table-icon 
                          +plus-svg.catalog-page__table-svg
    section.section 
      .container 
        +tech-docs